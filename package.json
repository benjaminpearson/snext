{
  "name": "sku-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vocab compile --watch & next dev",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm format-check && next lint",
    "format": "prettier --cache --write '**/*.{js,ts,tsx,md,json}' && eslint --cache --fix .",
    "format-check": "prettier --cache --list-different '**/*.{js,ts,tsx,md,json}'",
    "test": "jest --coverage",
    "test-watch": "pnpm test --watch",
    "test-e2e": "playwright test ./src/e2e/*.test.ts",
    "test-e2e-ui": "pnpm test-e2e --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --coverage",
    "test-storybook-watch": "test-storybook --watch",
    "test-storybook-ci": "pnpm dlx concurrently -k -s first -n \"Storybook,Test\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && pnpm dlx http-server storybook-static --port 6006 --silent\" \"pnpm dlx wait-on tcp:127.0.0.1:6006 && pnpm test-storybook --ci\"",
    "coverage-report-create": "cp coverage/storybook/coverage-storybook.json coverage/coverage-storybook.json && pnpm dlx nyc report --reporter=html -t coverage --report-dir coverage",
    "coverage-report-open": "open coverage/index.html"
  },
  "dependencies": {
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@vocab/core": "1.4.0",
    "@vocab/react": "1.1.7",
    "braid-design-system": "32.11.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "next-i18n-router": "3.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@playwright/test": "1.37.1",
    "@storybook/addon-coverage": "0.0.9",
    "@storybook/addon-essentials": "^7.4.2",
    "@storybook/addon-interactions": "^7.4.2",
    "@storybook/addon-links": "^7.4.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.2",
    "@storybook/jest": "0.2.2",
    "@storybook/nextjs": "^7.4.2",
    "@storybook/react": "^7.4.2",
    "@storybook/test-runner": "0.13.0",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.4",
    "@vanilla-extract/next-plugin": "2.3.0",
    "@vocab/cli": "1.3.7",
    "concurrently": "8.2.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-playwright": "0.16.0",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-testing-library": "6.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lefthook": "1.4.11",
    "nyc": "15.1.0",
    "prettier": "3.0.3",
    "storybook": "^7.4.2",
    "wait-on": "7.0.1"
  }
}
